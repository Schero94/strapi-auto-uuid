{
  "name": "strapi-auto-uuid",
  "version": "1.0.0",
  "description": "Strapi v5 plugin that automatically generates and manages UUID v4/v7 for your content types",
  "keywords": [
    "strapi",
    "strapi-plugin",
    "uuid",
    "uuid-v4",
    "uuid-v7",
    "strapi-v5",
    "custom-field",
    "unique-identifier"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "source": "./admin/src/index.js",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "source": "./server/src/index.js",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "strapi-admin.js",
    "strapi-server.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.2",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "@strapi/strapi": "^5.32.0",
    "@strapi/sdk-plugin": "^5.3.2",
    "prettier": "^3.7.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^7.1.14",
    "react-router-dom": "^6.30.1",
    "semantic-release": "^24.2.3",
    "styled-components": "^6.1.19"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.0.0",
    "@strapi/sdk-plugin": "^5.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "field-uuid",
    "displayName": "Auto UUID",
    "description": "Automatically generates and manages UUID v4/v7 for your content types with admin panel tools"
  },
  "license": "MIT",
  "author": "Cringe Studio / Schero D. <schero1894@gmail.com>",
  "homepage": "https://github.com/Schero94/strapi-auto-uuid#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Schero94/strapi-auto-uuid.git"
  },
  "bugs": {
    "url": "https://github.com/Schero94/strapi-auto-uuid/issues"
  },
  "private": false
}
